{
    "debug": true,
    "hostname": "",
    "ip": "",
    "plugin": {
        "enabled": false,
        "dir": "./plugin",
        "git": "https://github.com/open-falcon/plugin.git",
        "logs": "./logs"
    },
    "heartbeat": {
        "enabled": true,
        "addr": "127.0.0.1:6030",
        "interval": 60,
        "timeout": 1000
    },
    "transfer": {
        "enabled": true,
        "addrs": [
            "127.0.0.1:8433",
            "127.0.0.1:8433"
        ],
        "interval": 60,
        "timeout": 1000
    },
    "http": {
        "enabled": true,
        "listen": ":1988",
        "backdoor": false
    },
    "collector": {
        "ifacePrefix": ["eth", "em"]
    },
    "addTags":{
      "region": "cn"
    },
    "ignore": {
        "^cpu\\.busy": "",
        "^df\\.(bytes|inodes)\\.(total|free|used(\\.percent)?)": "",
        "^mem\\.(swap|mem)(total|free|used(\\.percent)?)": "",

        "^df\\.": "^.*mount=.*/docker/.*$",
    }
}
